<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JhoelSuarezPruebaProg2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:JhoelSuarezPruebaProg2.Views"
    Title="JhoelSuarezPruebaProg2">

    <FlyoutItem Title="Home" Icon="home.png">
        <ShellContent Route="Home" ContentTemplate="{DataTemplate views:Home}" />
    </FlyoutItem>

    <FlyoutItem Title="Persona" Icon="persona.png">
        <ShellContent Route="Persona" ContentTemplate="{DataTemplate views:Persona}" />
    </FlyoutItem>

    <FlyoutItem Title="Autos" Icon="car.png">
        <ShellContent Route="Autos" ContentTemplate="{DataTemplate views:Autos}" />
    </FlyoutItem>

    <FlyoutItem Title="Civil" Icon="civil.png">
        <ShellContent Route="Civil" ContentTemplate="{DataTemplate views:Civil}" />
    </FlyoutItem>

    <FlyoutItem Title="Legal" Icon="legal.png">
        <ShellContent Route="Legal" ContentTemplate="{DataTemplate views:Legal}" />
    </FlyoutItem>

    <FlyoutItem Title="Agregar Persona API" Icon="agg.png">
        <ShellContent Route="AgregarPersonaPage" ContentTemplate="{DataTemplate views:AgregarPersonaPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Datos Persona API" Icon="agg2.png">
        <ShellContent Route="DatosPersonaPage" ContentTemplate="{DataTemplate views:DatosPersonaPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Generar Reporte" >
        <ShellContent ContentTemplate="{DataTemplate views:Reporte}" />
    </FlyoutItem>
</Shell>
